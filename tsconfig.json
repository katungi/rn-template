{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "_app/*": ["src/app/*"],
      "_assets/*": ["src/assets/*"],
      "_components/*": ["src/components/*"],
      "_constants/*": ["src/constants/*"],
      "_context/*": ["src/context/*"],
      "_locales/*": ["src/locales/*"],
      "_utils/*": ["src/utils/*"],
      "_layouts/*": ["src/layouts/*"],
      "_config/*": ["src/config/*"]
    }
  }
}
